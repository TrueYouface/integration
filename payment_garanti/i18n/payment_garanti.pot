#. module: payment_garanti
#: model_terms:ir.ui.view,arch_db:payment_garanti.garanti_payment_result
msgid "<strong>Payment Status:</strong>\n"
"                                Awaiting payment. You can make your payments from\n"
"                                our sales portal."
msgstr "<strong>Ödeme Durumu:</strong>\n"
"                                Ödeme bekleniyor. Ödemelerinizi satış portalımızdan\n"
"                                yapabilirsiniz."

#. module: payment_garanti
#: model_terms:ir.ui.view,arch_db:payment_garanti.garanti_payment_result
msgid "<strong>Payment Status:</strong>\n"
"                                Canceled. Your payment has been canceled. Please\n"
"                                contact us or leave a message on sales portal."
msgstr "<strong>Ödeme Durumu:</strong>\n"
"                                İptal edildi. Ödemeniz iptal edildi. Lütfen bizimle\n"
"                                iletişime geçin veya satış portalına mesaj\n"
"                                bırakın."

#. module: payment_garanti
#: model_terms:ir.ui.view,arch_db:payment_garanti.garanti_payment_result
msgid "<strong>Payment Status:</strong>\n"
"                                Completed. Thank you for your payment. Your order\n"
"                                will be processed as soon as possible."
msgstr "<strong>Ödeme Durumu:</strong>\n"
"                                Tamamlandı. Ödemeniz için teşekkür ederiz. Siparişiniz\n"
"                                en kısa sürede işleme alınacaktır."

#. module: payment_garanti
#: model_terms:ir.ui.view,arch_db:payment_garanti.garanti_payment_result
msgid "<strong>Payment Status:</strong>\n"
"                                Error. Your payment could not be completed. Please\n"
"                                contact us or leave a message on sales portal."
msgstr "<strong>Ödeme Durumu:</strong>\n"
"                                Hata. Ödemeniz tamamlanamadı. Lütfen bizimle\n"
"                                iletişime geçin veya satış portalına mesaj\n"
"                                bırakın."

#. module: payment_garanti
#: model_terms:ir.ui.view,arch_db:payment_garanti.garanti_payment_result
msgid "<strong>Payment Status:</strong>\n"
"                                Pending. You can make your payments from our sales\n"
"                                portal."
msgstr "<strong>Ödeme Durumu:</strong>\n"
"                                Beklemede. Ödemelerinizi satış portalımızdan\n"
"                                yapabilirsiniz."

